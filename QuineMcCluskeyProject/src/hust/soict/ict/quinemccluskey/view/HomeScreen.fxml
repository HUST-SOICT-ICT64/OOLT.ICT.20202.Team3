<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hust.soict.ict.quinemccluskey.controller.home.HomeController">
   <top>
      <HBox prefHeight="125.0" prefWidth="1024.0" BorderPane.alignment="CENTER">
         <children>
            <Region prefHeight="200.0" prefWidth="200.0" />
            <Region prefHeight="200.0" prefWidth="200.0" />
            <ButtonBar>
              <buttons>
                <Button fx:id="threeVarsButton" mnemonicParsing="false" onAction="#threeVarsButtonPressed" text="3-variable" />
                  <Button fx:id="fourVarsButton" mnemonicParsing="false" onAction="#fourVarsButtonPressed" text="4-variable" />
              </buttons>
            </ButtonBar>
         </children>
      </HBox>
   </top>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Logic Circuit Simplifier">
               <font>
                  <Font size="50.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </center>
</BorderPane>
